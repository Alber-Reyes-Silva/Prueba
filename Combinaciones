def factorial(n):
    if n==0 or n==1:
        resultado=1
    elif n>1:
        resultado= n * factorial(n-1)
    return resultado

def combinaciones (n,m):
    return factorial(n)/(factorial(n-m)*factorial(m))


n1=int(input("Digite primer valor: "))
n2=int(input("Digite segundo valor: "))
print(combinaciones(n1,n2))
